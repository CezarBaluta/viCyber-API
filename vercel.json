
{
    "version": 2,
    "builds": [
        {
            "src": "*.js",   
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/",
            "dest": "/app.js"
        },
        {
            "src": "/admin",
            "dest": "/login.js"
        }
    ]
}